    <link rel="stylesheet" type="text/css" href="/assets/default/css/default.css" />
    {{ headLink() }}
    {{ headStyle() }}
{% endblock %}

{# DEFAULT HTML BODY CONTENT FOR ALL PAGES #}
{% block body %}
    {{ content | raw }}
{% endblock %}